<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="ownermapper">
	
	
    <select id="allowner" resultType="com.singsong.singsong.dto.Owner.Owner">
        select * from Owner;
    </select>

    <select id="getowner" resultType="com.singsong.singsong.dto.Owner.Owner">
        select * from Owner where o_id = #{id};
    </select>


    

	
    


</mapper>
